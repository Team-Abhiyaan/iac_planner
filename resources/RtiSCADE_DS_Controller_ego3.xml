                        nonBasicTypeName="IndyDS::RoadLinePolynom"/>
                <member name="timeOfUpdate" type="float64"/>
            </struct>
            <struct name="SensorTargetBBox">
                <member name="id" type="int16"/>
                <member name="referenceFrame" type="int16"/>
