            <topic name="radarSensorMovableTargets_R_ego3" register_type_ref="SensorMovableTargets"/>
            <topic name="radarSensorMovableTargets_Stbd_ego3" register_type_ref="SensorMovableTargets"/>
            <topic name="toVehicleCabToModelCorrective_ego3" register_type_ref="CabToModelCorrective"/>
