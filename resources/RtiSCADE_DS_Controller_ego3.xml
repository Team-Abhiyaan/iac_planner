        </domain>
    </domain_library>
    <!-- Participant library -->
    <domain_participant_library name="SCADE_DS_Controller">
        <domain_participant name="Controller" domain_ref="VRX::DS">

            <subscriber name="simWaitSub">
                <data_reader name="simWaitReader" topic_ref="dds_done3"/>
            </subscriber>
            <subscriber name="vehicleStateOutSub">
                <data_reader name="vehicleStateOutReader" topic_ref="vehicleStateVehicleOutput_ego3"/>
            </subscriber>
            <subscriber name="camRoadLinesF1Sub">
                <data_reader name="camRoadLinesF1Reader" topic_ref="cameraRoadLinesPolynoms_F1_ego3"/>
            </subscriber>
            <subscriber name="radarFSub">
                <data_reader name="radarFReader" topic_ref="radarSensorMovableTargets_F_ego3"/>
            </subscriber>

            <publisher name="toVehicleModelCorrectivePub">
                <data_writer name="toVehicleModelCorrectiveWriter"
                             topic_ref="toVehicleCabToModelCorrective_ego3"/>
            </publisher>
            <publisher name="toVehicleSteeringPub">
                <data_writer name="toVehicleSteeringWriter" topic_ref="toVehicleSteeringCorrective_ego3"/>
            </publisher>
            <publisher name="toSimDonePub">
                <data_writer name="toSimDoneWriter" topic_ref="dds_done_reply3"/>
            </publisher>

        </domain_participant>
